<html>
	<head>
		<title>Orhpeos Login</title>
		<% include ../templates/headTemplate.ejs %>
	</head>

	<body>
		<div>
            <p>
                ID: <%= user.id %><br/>
                Username: <%= user.username %><br/>
                Name: <%= user.displayName %><br/>
                <% if (user.emails) { %>
                Email: <%= user.emails[0].value %><br/>
                <% } %>
            </p>
            <a href="/logout">Log out</a>
		</div>
	</body>
</html>
